        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Handle logic

if (logic)
{

if (alarm[5] = -1)
{
if (dir = "w")
   {
    x = oPlayer.x;         
    if (distance_to_object(oPlayer) &lt; 250) 
    {
    if (place_free(x,y-16)) {y-=16;}
     }
     
    for (a = 0; a &lt; point_distance(x,y,oPlayer.x,oPlayer.y) / 8; a++)
        {
         effectFiretrail(x,y+a*8,1);
        }
    
   }
if (dir = "s")
   {
     x = oPlayer.x;         
    if (distance_to_object(oPlayer) &lt; 250) 
    {
    if (place_free(x,y+16)) {y+=16;}
     }
     
    for (a = 0; a &lt; point_distance(x,y,oPlayer.x,oPlayer.y) / 8; a++)
        {
         effectFiretrail(x,y-a*8,1);
        }
    

   }
if (dir = "a")
   {
     y = oPlayer.y;         
    if (distance_to_object(oPlayer) &lt; 250) 
    {
    if (place_free(x-16,y)) {x-=16;}
     }
     
    for (a = 0; a &lt; point_distance(x,y,oPlayer.x,oPlayer.y) / 8; a++)
        {
         effectFiretrail(x+a*8,y,1);
        }
    

   }
if (dir = "d")
   {
    y = oPlayer.y;         
